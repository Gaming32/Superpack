    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Superpack",
            "request": "launch",
            "mainClass": "io.github.gaming32.superpack.Superpack",
            "projectName": "super-launcher",
            "vmArgs": [
                "-Dsuperpack.debug=true"
            ]
        }
    ]
}